{
    "name": "medicalmundi/memu-job",
    "type": "project",
    "description": "job board",
    "license": "proprietary",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 || ^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.*",
        "debril/feed-io": "^4.9",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "league/fractal": "^0.20.1",
        "ramsey/uuid": "^4.1",
        "ramsey/uuid-doctrine": "^1.8",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.15.4",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0",
        "webmozart/assert": "^1.11"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "dama/doctrine-test-bundle": "^7.0",
        "dbrekelmans/bdi": "^1.0",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.6",
        "friends-of-behat/symfony-extension": "^2.3",
        "phpunit/phpunit": "^9.5",
        "robertfausk/behat-panther-extension": "^1.1",
        "soyuka/contexts": "^3.3",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/panther": "^1.1",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ingesting\\": "core/ingesting/src/",
            "Publishing\\": "core/publishing/src/",
            "Backoffice\\": "core/backoffice/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Ingesting\\Tests\\": "core/ingesting/tests/",
            "Publishing\\Tests\\": "core/publishing/tests/",
            "Backoffice\\Tests\\": "core/backoffice/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
